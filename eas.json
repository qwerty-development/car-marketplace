{
  "cli": {
    "version": ">= 12.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "android": {
        "buildType": "app-bundle", 
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {}
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_PROJECT_ID": "aaf80aae-b9fd-4c39-a48a-79f2eac06e68"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_PROJECT_ID": "aaf80aae-b9fd-4c39-a48a-79f2eac06e68"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_PROJECT_ID": "aaf80aae-b9fd-4c39-a48a-79f2eac06e68"
      }
    }
  },
  "submit": {
    "production": {}
  }
}