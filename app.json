{
  "expo": {
    "name": "Fleet",
    "slug": "clerk-expo-quickstart",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "fleet",
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/images/app-icon.png",
      "color": "#D55004",
      "sound": "./assets/sounds/notification.wav",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New Cars Available!"
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "translucent": true,
      "barStyle": "light-content",
      "hidden": false
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.qwertyapp.clerkexpoquickstart",
      "associatedDomains": [
        "applinks:fleetapp.me",
        "applinks:www.fleetapp.me"
      ],
      "splash": {
        "image": "./assets/background-white.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "cover",
          "image": "./assets/background-black.png"
        }
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDvW1iMajBuW0mqJHIyNFtDm8A7VkgkAdg"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "We need your location to find nearby dealerships.",
        "LSApplicationQueriesSchemes": [
          "whatsapp",
          "comgooglemaps",
          "googlechromes"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.qwertyapp.clerkexpoquickstart",
              "fleet"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "minSdkVersion": 21,
      "splash": {
        "image": "./assets/background-white.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCDuRjdx7YfYc0Y46fcEisE6YbY0zVY7jk"
        }
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.qwertyapp.clerkexpoquickstart",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.fleetapp.me",
              "pathPrefix": "/cars"
            },
            {
              "scheme": "https",
              "host": "fleetapp.me",
              "pathPrefix": "/cars"
            },
            {
              "scheme": "fleet",
              "host": "*",
              "pathPrefix": "/cars"
            },
            {
              "scheme": "https",
              "host": "www.fleetapp.me",
              "pathPrefix": "/clips"
            },
            {
              "scheme": "https",
              "host": "fleetapp.me",
              "pathPrefix": "/clips"
            },
            {
              "scheme": "fleet",
              "host": "*",
              "pathPrefix": "/clips"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "fleet"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./plugins/withCustomProguardRules",
      "./plugins/withCustomSplashScreen",
      "./plugins/withCopySplashAssets",
      "./plugins/withSplashScreenColors",
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload car images."
        }
      ],
      [
        "expo-file-system",
        {
          "filePermission": "The app needs access to files for image uploads."
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#D55004",
          "sounds": [
            "./assets/sounds/notification.wav"
          ],
          "mode": "production"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "fleet",
          "organization": "qwerty-5v"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "aaf80aae-b9fd-4c39-a48a-79f2eac06e68"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/aaf80aae-b9fd-4c39-a48a-79f2eac06e68"
    },
    "owner": "qwerty-app"
  }
}
